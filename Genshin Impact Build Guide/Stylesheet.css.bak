@font-face {
	font-family: genshin;
	src: url("assets/genshin_font.ttf");
}
body {
	background-color: #1c1524; 
	color: #e6e6e6;
	margin: 150px;
	font-family: genshin;
} 
h1 {
	font-size: 75px;
	font-family: genshin;
	border-bottom: 5px solid;
}
a {
	color: #ae73bd; 
	text-decoration: none;
	active-color: #8b499c;
	hover-color: #e7bbf2;
}
p.footnote {
	color: #efdff5;
	font-size: 12px;
	font-family: genshin;
}
li.footnote {
	color: #efdff5;
	font-size: 12px;
	font-family: genshin;
}
h2 {
	font-size: 18px;
	margin-left: 12px;
}
h3 {
	color: #ffd9fa;
	font-size: 18px;
	font-family: genshin;
	border-bottom: 2px solid;
}
h4{
	font-size: 14px;
	font-family: genshin;
}
em {
	color: #e4aee6;
	font-family: genshin;
}
div {
	border: 1px solid;
	border-radius: 5px;
	padding: 5px;
	background-color: #281b38;
}
div.ToC {
	border: 1px solid;
	border-radius: 5px;
	padding: 5px;
	background-color: #281b38;
	margin-right: 80%;
}
